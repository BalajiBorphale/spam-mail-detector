{
  "manifest_version": 3,
  "name": "SMDetector",
  "description": "Spam mail detector using Naive Bayes",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "storage", "scripting"]
}

